{
	"name": "tmdl-extension",
	"description": "A Tabular Model Definition Language (TMDL) extension for Visual Studio Code.",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"version": "0.2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode-tmdl"
	},
	"publisher": "analysis-services",
	"categories": [],
	"keywords": [
		"multi-root ready"
	],
	"engines": {
		"vscode": "^1.74.0"
	},
	"contributes": {
		"languages": [
			{
			   "id": "tmdl",
			   "aliases": [
					"Tabular Model Definition Language",
					"TMDL",
					"tmdl"
			   ],
			   "extensions": [ ".tmdl", ".tmd" ],
			   "configuration": "./config/tmdl-configuration.json"
			}
		 ],
		 "grammars": [
			{
			  "language": "tmdl",
			  "scopeName": "source.tmdl",
			  "path": "./syntaxes/tmdl.tmLanguage.json"
			}
		]
	}
}
