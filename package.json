{
	"name": "TMDL",
	"version": "0.0.1",
	"private": false,
	"description": "Language support for the Tabular Model Definition Language (TMDL).",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"contributes": {
		"grammars": [
			{
				"language": "tmdl",
				"path": "./syntaxes/tmdl.tmLanguage.json",
				"scopeName": "source.tmdl"
			}
		],
		"languages": [
			{
				"aliases": [
					"Tabular Model Definition Language",
					"TMDL",
					"tmdl"
				],
				"configuration": "./config/tmdl-configuration.json",
				"extensions": [
					".tmdl",
					".tmd"
				],
				"id": "tmdl"
			}
		]
	},
	"devDependencies": {
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	}
}
