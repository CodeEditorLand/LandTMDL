{
	"name": "TMDL",
	"version": "0.0.1",
	"private": false,
	"description": "Language support for the Tabular Model Definition Language (TMDL).",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandTMDL#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandTMDL/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandTMDL.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"grammars": [
			{
				"language": "tmdl",
				"path": "./syntaxes/tmdl.tmLanguage.json",
				"scopeName": "source.tmdl"
			}
		],
		"languages": [
			{
				"aliases": [
					"Tabular Model Definition Language",
					"TMDL",
					"tmdl"
				],
				"configuration": "./config/tmdl-configuration.json",
				"extensions": [
					".tmdl",
					".tmd"
				],
				"id": "tmdl"
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	}
}
